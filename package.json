    "@types/dotenv": "^6.1.1",
    "@types/hapi__hapi": "^18.2.1",
    "@types/hapi__inert": "^5.2.0",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.17.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
